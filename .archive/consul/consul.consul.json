{
  "service": {
    "id": "consul",
    "name": "consul",
    "tags": ["configuration","discovery","consul"],
    "address": "consul",
    "port": 8500,
    "meta": {
      "prometheus_path": "/consul/metrics"
    },
    "check": {
      "id": "consul-http-check",
      "name": "consul Health Check",
      "http": "http://consul:8500/v1/status/leader",
      "interval": "30s",
      "timeout": "5s"
    }
  }
}