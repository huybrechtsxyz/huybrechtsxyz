{
  "server": true,
  "client_addr": "0.0.0.0",
  "ui_config": {
    "enabled": true
  },
  "retry_join": ["provider=docker"],
  "log_level": "DEBUG",
  "ports": { "dns": 8600, "http": 8500 },
  "recursors": [ "10.0.0.10" ],
  "service": {
    "id": "consul",
    "name": "consul",
    "address": "consul",
    "port": 8500,
    "meta": {
      "consul_path": "/"
    },
    "checks": [
      {
        "id": "consul-http-check",
        "name": "Consul HTTP Health Check",
        "http":  "http://localhost:8500/v1/status/leader",
        "interval": "15s",
        "timeout": "5s"
      }
    ],
    "tags":[ "consul","dns","discovery","configuration","docker-swarm","system" ]
  }
}
