{
  "services": [
    {
      "id": "authentik-server",
      "name": "authentik-server",
      "address": "authentik-server",
      "port": 8000,
      "checks": [
        {
          "id": "authentik-server-http-check",
          "name": "Authentik Server TCP Health Check",
          "http": "http://authentik-server:8000/",
          "interval": "15s",
          "timeout": "5s"
        }
      ],
      "tags":[ "redis","caching","docker-swarm","system" ]
    },
    {
      "id": "redisinsight",
      "name": "redisinsight",
      "address": "redisinsight",
      "port": 8001,
      "meta": {
        "redis_path": "/redis"
      },
      "checks": [
        {
          "id": "redisinsight-http-check",
          "name": "Redis Insight HTTP Health Check",
          "http": "http://redisinsight:8001/",
          "interval": "15s",
          "timeout": "5s"
        }
      ],
      "tags":[ "redis","caching","docker-swarm","system" ]
    }
  ]
}