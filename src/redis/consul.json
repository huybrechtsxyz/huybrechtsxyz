{
  "service": {
    "id": "redis",
    "name": "Redis",
    "address": "redis",
    "port": 6379,
    "checks": [
      {
        "id": "redis-tcp-check",
        "name": "Redis TCP Health Check",
        "tcp": "redis:6379",
        "interval": "60s",
        "timeout": "15s"
      }
    ],
    "tags":[ "redis","caching","docker-swarm","system" ]
  }
}