{
  "id": "$REALM_ID",
  "realm": "$REALM_ID",
  "enabled": true,
  "displayName": "$REALM_NAME",
  "displayNameHtml": "$REALM_NAME",
  "clients": [
    {
      "clientId": "$TRAEFIK_CLIENTID",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "$TRAEFIK_SECRET",
      "redirectUris": [
        "https://traefik.$DOMAIN_DEV/_oauth"
      ],
      "webOrigins": [
        "https://traefik.$DOMAIN_DEV"
      ],
      "postLogoutRedirectUris": [
        "https://traefik.$DOMAIN_DEV"
      ],
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "serviceAccountsEnabled": true
    }
  ]
}