{
  "services": [
    {
      "id": "prometheus",
      "name": "prometheus",
      "tags": ["prometheus","metrics"],
      "address": "prometheus",
      "port": 9090,
      "meta": {
        "prometheus_path": "/metrics"
      },
      "check": {
        "id": "prometheus-http-check",
        "name": "Prometheus HTTP Health Check",
        "http": "http://prometheus:9090/-/ready",
        "interval": "15s",
        "timeout": "5s"
      }
    },
    {
      "id": "grafana",
      "name": "grafana",
      "address": "grafana",
      "port": 3000,
      "meta": {},
      "checks": [
        {
          "id": "grafana-http-check",
          "name": "Grafana Health Check",
          "http": "http://grafana:3000/health/healthz",
          "interval": "30s",
          "timeout": "5s"
        }
      ],
      "tags": [ "grafana","logging","metrics","tracing","docker-swarm","system" ]
    }
  ]
}