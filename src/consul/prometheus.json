{
  "service": {
    "id": "prometheus-1",
    "name": "prometheus",
    "tags": ["monitoring", "prometheus"],
    "port": 9090,
    "checks": [
      {
        "id": "traefik-http-check",
        "name": "Traefik HTTP Health Check",
        "http": "http://prometheus:9090/metrics",
        "interval": "15s",
        "timeout": "5s"
      }
    ]
  }
}