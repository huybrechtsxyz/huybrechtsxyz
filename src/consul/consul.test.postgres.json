{
  "service": {
    "name": "postgres",
    "port": 5432,
    "checks": [
      {
        "id": "postgres-tcp-check",
        "name": "PostgreSql TCP Health Check",
        "tcp": "postgres:5432",
        "interval": "10s",
        "timeout": "1s"
      }
    ],
    "tags": [
      "postgres","storage","data-store","data","docker-swarm","system",
      "- traefik.enable=false"
    ]
  }
}