{
  "server": true,
  "bind_addr": "0.0.0.0",
  "client_addr": "0.0.0.0",
  "retry_join": ["provider=docker"],
  "ui_config": {
    "enabled": true
  },
  "log_level": "INFO",
  "ports": {
    "http": 8500,
    "dns": 8600
  },
  "telemetry": {
    "prometheus_retention_time": "5m"
  },
  "service": {
    "id": "consul",
    "name": "consul",
    "address": "127.0.0.1",
    "port": 8500,
    "tags": ["consul", "dns", "discovery", "configuration", "docker-swarm", "system"],
    "meta": {
      "consul_path": "/"
    },
    "checks": [
      {
        "id": "consul-http-check",
        "name": "Consul HTTP Health Check",
        "http": "http://localhost:8500/v1/status/leader",
        "interval": "60s",
        "timeout": "15s"
      }
    ]
  }
}