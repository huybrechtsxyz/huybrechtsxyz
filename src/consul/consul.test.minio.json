{
  "services": [
    {
      "id": "minio-data",
      "name": "minio-data",
      "address": "minio-data",
      "port": 9000,
      "meta": {
        "prometheus_path": "/minio/metrics"
      },
      "checks": [
        {
          "id": "minio-http-check",
          "name": "MinIO Health Check",
          "http": "http://minio:9000/minio/health/live",
          "interval": "30s",
          "timeout": "5s"
        }
      ],
      "tags": [
        "minio","storage","object-store","data","docker-swarm","system",
        "- traefik.enable=true",
        "- traefik.tags=test",
        "- traefik.http.routers.minio-data.rule=Host(`data.test.huybrechts.dev`)",
        "- traefik.http.routers.minio-data.entrypoints=websecure",
        "- traefik.http.routers.minio-data.tls.certresolver=leresolver",
        "- traefik.http.services.minio-data.loadbalancer.server.scheme=http",
        "- traefik.http.services.minio-data.loadbalancer.server.port=9000"
      ]
    }
  ]
}