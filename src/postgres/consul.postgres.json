{
  "services": {
    "id": "pgadmin",
    "name": "pgadmin",
    "address": "pgadmin",
    "port": 9080,
    "meta": {
      "priority": 3,
      "groups": ["infra","sqldb"],
      "description": "PostgreSQL database service for data storage and retrieval",
      "endpoint": "",
      "paths": [
        { "path":"conf", "chmod": 755 },
        { "path":"data", "chmod": 777 },
        { "path":"backups", "chmod": 777 }
      ],
      "project":{
        "name": "",
        "file": "",
        "registry": "",
        "imagename": "",
        "dockerfile": "",
        "buildconfig": "",
        "runtime": "",
        "runversion": ""
      }
    },
    "checks": [
      {
        "id": "postgres-tcp-check",
        "name": "PostgreSql TCP Health Check",
        "tcp": "postgres:5432",
        "interval": "60s",
        "timeout": "15s"
      }
    ],
    "tags": [ "postgres","storage","data-store","data","docker-swarm","system" ]
  }
}