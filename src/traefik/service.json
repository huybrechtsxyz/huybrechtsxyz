{
  "service": {
    "id": "traefik",
    "name": "Traefik",
    "address": "traefik",
    "port": 8080,
    "meta": {
      "priority": 1,
      "groups": ["infra"],
      "description": "Reverse proxy and load balancer for microservices",
      "endpoint": "http://traefik.${DOMAIN_DEV}/dashboard/",
      "paths": [
        { "path":"conf", "chmod": 755 },
        { "path":"data", "chmod": 600 },
        { "path":"logs", "chmod": 777 }
      ],
      "project":{
        "name": "",
        "file": "",
        "registry": "",
        "imagename": "",
        "dockerfile": "",
        "buildconfig": "",
        "runtime": "",
        "runversion": ""
      },
      "traefik_path": "/ping"
    },
    "checks": [
      {
        "id": "traefik-http-check",
        "name": "Traefik HTTP Health Check",
        "http": "http://traefik:8080/ping",
        "interval": "60s",
        "timeout": "15s"
      }
    ],
    "tags":[ "traefik","proxy","docker-swarm","system" ]
  }
}